<core:View
    controllerName="app.view.ServiceCoverage"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core">

    <Page
        id="page"
        title="Here map with composite clustering">
        <content>
            <VBox
                width="100%"
                direction="Column">
                <items>
                    <HBox
                        justifyContent="SpaceAround"
                        alignItems="Center">
                        <layoutData>
                            <FlexItemData
                                alignSelf="Stretch">
                            </FlexItemData>
                        </layoutData>
                        <items>
                            <HBox>
                                <layoutData>
                                    <FlexItemData
                                        growFactor="1">
                                    </FlexItemData>
                                </layoutData>
                                <items>
                                    <Button
                                        class="appdesignerDraggable"
                                        text="I do nothing!"
                                        width="100%"
                                        press="onButtonPress">
                                    </Button>
                                </items>
                            </HBox>
                            <HBox
                                justifyContent="End"
                                alignItems="End">
                                <layoutData>
                                    <FlexItemData>
                                    </FlexItemData>
                                </layoutData>
                                <items>
                                    <Button
                                        type="Transparent"
                                        icon="sap-icon://table-chart"
                                        press="handleNav">
                                        <layoutData>
                                          <FlexItemData growFactor="1" />
                                        </layoutData>
                                        <customData>
                                          <core:CustomData
                                            key="target"
                                            value="CoverageMap" />
                                        </customData>
                                    </Button>
                                </items>
                            </HBox>
                        </items>
                    </HBox>
                    <NavContainer
                      id="CoverageContainer"
                      height="600px"
                      class="navContainerControl">
                    <mvc:XMLView
                        height="100%"
                        id="CoverageMap"
                        viewName="app.view.CoverageMap">
                    </mvc:XMLView>
                      <mvc:XMLView
                        id="CoverageReport"
                        viewName="app.view.CoverageReport">
                      </mvc:XMLView>

                    </NavContainer>

                </items>
            </VBox>
        </content>
    </Page>
</core:View>