<core:View
    controllerName="app.view.ServiceCoverage"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core">

    <Page
        id="page"
        title="Blank page">
        <content>
            <VBox
                width="100%"
                direction="Column">
                <items>
                    <HBox
                        justifyContent="SpaceAround"
                        alignItems="Center">
                        <layoutData>
                            <FlexItemData
                                alignSelf="Stretch">
                            </FlexItemData>
                        </layoutData>
                        <items>
                            <HBox>
                                <layoutData>
                                    <FlexItemData
                                        growFactor="1">
                                    </FlexItemData>
                                </layoutData>
                                <items>
                                    <Button
                                        class="appdesignerDraggable"
                                        text="page2"
                                        width="100%"
                                        press="onButtonPress">
                                    </Button>
                                </items>
                            </HBox>
                            <HBox
                                justifyContent="End"
                                alignItems="End">
                                <layoutData>
                                    <FlexItemData>
                                    </FlexItemData>
                                </layoutData>
                                <items>
                                    <Button
                                        type="Transparent"
                                        icon="sap-icon://globe"
                                        press="onCoverageToggle">
                                    </Button>
                                </items>
                            </HBox>
                        </items>
                    </HBox>
                    <VBox id="CoverageContainer">
<!--                         <items>
                            <mvc:XMLView
                                height="600px"
                                id="CoverageMap"
                                viewName="app.view.CoverageMap">
                            </mvc:XMLView>
                        </items> -->
                    </VBox>
                </items>
            </VBox>
        </content>
    </Page>
</core:View>